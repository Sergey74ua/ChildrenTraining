  </ItemGroup>

  <ItemGroup>
    <Resource Include="data\MainWindow.xaml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="data\Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>
